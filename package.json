{
  "name": "vue-cute-timeline",
  "version": "1.2.8",
  "description": "A cute timeline component for Vue.js",
  "repository": {
    "url": "luyilin/vue-cute-timeline",
    "type": "git"
  },
  "main": "dist/vue-cute-timeline.js",
  "files": [
    "dist"
  ],
  "module": "bili",
  "keywords": [
    "timeline",
    "vue-timeline",
    "vue-cute-timeline"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build && npm run build:example",
    "lint": "eslint . --ext .js --ext .vue",
    "test": "npm run lint && test-vue-app",
    "build": "bili --no-babelrc",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": {
    "name": "luyilin",
    "email": "luyilin12@gmail.com"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/recommended"
    ],
    "rules": {
      "vue/max-attributes-per-line": [
        2,
        {
          "singleline": 4,
          "multiline": {
            "max": 4,
            "allowFirstLine": true
          }
        }
      ]
    }
  },
  "devDependencies": {
    "bili": "^4.10.1",
    "eslint": "^4.17.0",
    "eslint-plugin-vue": "^4.2.2",
    "poi": "^9.3.10",
    "rollup-plugin-vue": "^5.0.0",
    "test-vue-app": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "peerDependencies": {
    "vue": "^2.1.8"
  }
}
